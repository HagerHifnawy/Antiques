<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.antiqques">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo_launcher_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".pojo.Search"></activity>
        <activity android:name=".ui.registration.Resetpassword" />
        <activity
            android:name=".pojo.CategoryActivity"
            android:label="@string/title_activity_category"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".pojo.Setting" />
        <activity android:name=".ui.registration.ForgetPassword" />
        <activity android:name=".ui.registration.SignUp" />
        <activity android:name=".ui.registration.Login" />
        <activity android:name=".ui.intro.Intro">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".pojo.MainActivity" />
    </application>

</manifest>